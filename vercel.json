{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
    },
  "env": {
    "NODE_ENV": "production",
    "CORS_ORIGINS": "https://fundhubui-v1.vercel.app,http://localhost:5173"
  },
  "outputDirectory": "."
}
